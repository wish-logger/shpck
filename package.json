    "commander": "^11.1.0",
    "ffmpeg": "^0.0.4",
    "filesize": "^10.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^10.3.10",
    "mime-types": "^2.1.35",
    "ora": "^5.4.1",
    "progress": "^2.0.3",
    "sharp": "^0.33.5"
